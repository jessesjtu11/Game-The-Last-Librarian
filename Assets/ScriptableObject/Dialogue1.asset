  dialogueSegments:
  - backgroundImage: {fileID: 21300000, guid: 77ab16aaae291224c8f740c0fe8ee78c, type: 3}
    dialogueText: "\u516C\u514322\u4E16\u7EAA\uFF0C\u4EBA\u7C7B\u7684\u8D2A\u5A6A\u4E0E\u611A\u6627\u70B9\u71C3\u4E86\u6BC1\u706D\u7684\u5BFC\u706B\u7D22\u3002\u6838\u6218\u4E89\u7206\u53D1\uFF0C\u6570\u4EE5\u4E07\u8BA1\u7684\u6838\u5F39\u5728\u4E16\u754C\u5404\u5730\u7206\u70B8\uFF0C\u66FE\u7ECF\u7E41\u534E\u7684\u4E16\u754C\u77AC\u95F4\u6CA6\u4E3A\u5E9F\u589F\u3002"
    textSpeed: 0.1
  - backgroundImage: {fileID: 21300000, guid: df2790ecb8788b14197bbe655aed512f, type: 3}
    dialogueText: "\u6838\u6218\u4E89\u7684\u4F59\u6CE2\u5E26\u6765\u4E86\u6781\u5BD2\u7684\u6C14\u5019\uFF0C\u6C14\u6E29\u9AA4\u964D\u81F3\u96F6\u4E0B40\xB0C\u3002\u5730\u8868\u88AB\u539A\u539A\u7684\u8F90\u5C04\u4E91\u8986\u76D6\uFF0C\u4EBA\u7C7B\u5728\u6781\u5BD2\u7684\u5BA4\u5916\u5BF8\u6B65\u96BE\u884C\uFF0C\u6587\u660E\u7684\u706B\u79CD\u4F3C\u4E4E\u5373\u5C06\u7184\u706D\u3002"
    textSpeed: 0.1
  - backgroundImage: {fileID: 21300000, guid: ce25e1dada208dd4f83c26e8b3fd91e2, type: 3}
    dialogueText: "\u4F60\uFF0C\u4E00\u4E2A\u56FE\u4E66\u7BA1\u7406\u5458\uFF0C\u88AB\u56F0\u5728\u4E86\u8FD9\u5EA7\u56FE\u4E66\u9986\u4E2D\u3002\u5916\u9762\u7684\u6781\u5BD2\u548C\u8F90\u5C04\u8BA9\u4F60\u65E0\u6CD5\u79BB\u5F00\uFF0C\u56FE\u4E66\u9986\u6210\u4E86\u4F60\u552F\u4E00\u7684\u907F\u96BE\u6240\u3002"
    textSpeed: 0.1
  - backgroundImage: {fileID: 21300000, guid: e2cf7476f4fbe404c82148ecf4140040, type: 3}
    dialogueText: "\u5728\u8FD9\u672B\u4E16\u7684\u907F\u96BE\u6240\u4E2D\uFF0C\u4F60\u9762\u4E34\u7740\u8270\u96BE\u7684\u6289\u62E9\uFF1A\u662F\u5C06\u4E66\u7C4D\u6295\u5165\u706B\u5806\uFF0C\u6362\u53D6\u7247\u523B\u7684\u6E29\u6696\uFF0C\u8FD8\u662F\u5728\u9965\u5BD2\u4EA4\u8FEB\u4E2D\u575A\u6301\u9605\u8BFB\uFF0C\u4ECE\u77E5\u8BC6\u4E2D\u5BFB\u627E\u751F\u5B58\u7684\u5E0C\u671B\uFF1F"
    textSpeed: 0.1
  - backgroundImage: {fileID: 21300000, guid: ac8e52ba534dc7544893493dc10e347f, type: 3}
    dialogueText: "\u5F53\u7136\uFF0C\u751F\u5B58\u662F\u7B2C\u4E00\u8981\u52A1\u3002\u4F46\u5F53\u4F60\u9009\u62E9\u4E86\u77E5\u8BC6\uFF0C\u9009\u62E9\u4E86\u6210\u957F\uFF0C\u6216\u8BB8\u4F60\u7684\u547D\u8FD0\u3001\u751A\u81F3\u662F\u5168\u4EBA\u7C7B\u7684\u547D\u8FD0\uFF0C\u90FD\u5C06\u968F\u4E4B\u6539\u53D8\u3002\u800C\u8FD9\u4E00\u5207\uFF0C\u90FD\u4ECE\u4F60\u624B\u4E2D\u7684\u4E66\u7C4D\u5F00\u59CB\u2026\u2026"
    textSpeed: 0.1
